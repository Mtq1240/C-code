#include<stdio.h>
void Swap(int* pa, int* pb)     //*p读取到a和b的地址,改变变量的地址要运用指针,外部和内部函数的地址不一样会发生改变
{
	int z = 0;
	z = *pa;
	*pa = *pb;
	*pb = z;

}
int main()
{
	int a = 10;
	int b = 20;
	printf("交换前 a=%d ,b=%d \n", a, b);
	Swap(&a, &b);    //取地址a和b,运用指针变量
	printf("交换后 a=%d ,b=%d \n",a,b);
	return 0;




//1.函数打印100~200之间的素数
int is_prime(t)
{
	int n = 0;
	for (n = 2; n < t; n++)
	{				//2~n-1之间的数字试除i
		if (t%n==0)
		{
			return 0;
		}
		return 1;
	}
}
#include<stdio.h>
int main()
{
				//100~200之间的素数
	int i = 0;
	int count = 0;
	for (i = 100; i <= 200; i++)
	{
				//判断i是否为素数
		if (is_prime(i) == 1)
		{
			count++;
			printf("%d ", i);
		}
	}
	printf("\ncount=%d\n", count);
	return 0;
}

#include<stdio.h>
void Swap(int* pa, int* pb)     //*p读取到a和b的地址,改变变量的地址要运用指针,外部和内部函数的地址不一样会发生改变
{
	int z = 0;
	z = *pa;
	*pa = *pb;
	*pb = z;

}
int main()
{
	int a = 10;
	int b = 20;
	printf("交换前 a=%d ,b=%d \n", a, b);
	Swap(&a, &b);    //取地址a和b,运用指针变量
	printf("交换后 a=%d ,b=%d \n",a,b);
	return 0;



1.

#include<stdio.h>
int main()
{
	int ch = 0;
	while ((ch = getchar()) != EOF)                                 //EOF->end of file -> -1    
                                                                  //ctrl+z
	{	
		if (ch < '0' || ch>'9')
			continue;
		putchar(ch);
	}
	return 0;
}
